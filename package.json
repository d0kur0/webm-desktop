	"description": "My Electron application description",
	"main": "electron/index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run start-vite\" \"npm run start-forge\"",
		"start-forge": "electron-forge start",
		"start-vite": "vite",
